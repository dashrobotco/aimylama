{
    "version": 2,
    "builds": [
      { "src": "public/**", "use": "@vercel/static" },
      { "src": "api/**/*.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/socketio", "dest": "/api/socketio.js" },
      { "src": "/api/socketio/(.*)", "dest": "/api/socketio.js" },
      { "src": "/(.*)", "dest": "/public/$1" }
    ]
  }